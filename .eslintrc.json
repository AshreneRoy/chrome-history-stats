{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/prefer-default-export": "off",
        "import/extensions": [
            "error",
            {
                "js": "always"
            }
        ]
    }
}
